{
    "type": "robo-tasks",
    "name": "sweetchuck/robo-hash",
    "description": "Create and validate checksums",
    "keywords": [
        "robo-tasks",
        "checksum",
        "hash"
    ],
    "authors": [
        {
            "name": "DÃ¡vid Andor",
            "role": "Maintainer",
            "homepage": "https://github.com/Sweetchuck"
        }
    ],
    "homepage": "https://github.com/Sweetchuck/robo-hash",
    "support": {
        "source": "https://github.com/Sweetchuck/robo-hash",
        "issues": "https://github.com/Sweetchuck/robo-hash/issues"
    },
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "sweetchuck/git-hooks": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": ">=8.2",
        "ext-hash": "*",
        "consolidation/robo": "^4.0"
    },
    "require-dev": {
        "ext-json": "*",
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0",
        "nuvoleweb/robo-config": "^3.0",
        "squizlabs/php_codesniffer": "^3.5",
        "sweetchuck/codeception-module-robo-task-runner": "3.x-dev",
        "sweetchuck/git-hooks": "^2.x-dev",
        "sweetchuck/robo-git": "3.x-dev",
        "sweetchuck/robo-phpcs": "3.x-dev",
        "sweetchuck/robo-phpmd": "3.x-dev",
        "symfony/error-handler": "^5.0 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Sweetchuck\\Robo\\Hash\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweetchuck\\Robo\\Hash\\Tests\\Acceptance\\": "tests/acceptance/",
            "Sweetchuck\\Robo\\Hash\\Tests\\Unit\\": "tests/unit/",
            "Sweetchuck\\Robo\\Hash\\Tests\\": "tests/_support/"
        }
    },
    "scripts": {},
    "scripts-descriptions": {}
}
