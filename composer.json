{
    "type": "robo-tasks",
    "name": "sweetchuck/robo-hash",
    "description": "Create and validate checksums",
    "keywords": [
        "robo-tasks",
        "checksum",
        "hash"
    ],
    "authors": [
        {
            "name": "DÃ¡vid Andor",
            "role": "Maintainer",
            "homepage": "https://github.com/Sweetchuck"
        }
    ],
    "homepage": "https://github.com/Sweetchuck/robo-hash",
    "support": {
        "source": "https://github.com/Sweetchuck/robo-hash",
        "issues": "https://github.com/Sweetchuck/robo-hash/issues"
    },
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "sweetchuck/git-hooks": true
        }
    },
    "repositories": {
        "nuvoleweb/robo-config": {
            "type": "github",
            "url": "https://github.com/Sweetchuck/robo-config.git"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-hash": "*",
        "consolidation/robo": "^3.0"
    },
    "require-dev": {
        "ext-json": "*",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.1",
        "nuvoleweb/robo-config": "3.x-dev",
        "psr/container": "^1.0",
        "squizlabs/php_codesniffer": "^3.5",
        "sweetchuck/codeception-module-robo-task-runner": "2.x-dev",
        "sweetchuck/git-hooks": "^2.x-dev",
        "sweetchuck/robo-git": "^2.x-dev",
        "sweetchuck/robo-phpcs": "^2.x-dev",
        "sweetchuck/robo-phpmd": "^2.x-dev",
        "sweetchuck/utils": "1.x-dev",
        "symfony/error-handler": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Sweetchuck\\Robo\\Hash\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweetchuck\\Robo\\Hash\\Tests\\Acceptance\\": "tests/acceptance/",
            "Sweetchuck\\Robo\\Hash\\Tests\\Unit\\": "tests/unit/",
            "Sweetchuck\\Robo\\Hash\\Tests\\": "tests/_support/"
        }
    },
    "scripts": {},
    "scripts-descriptions": {}
}
